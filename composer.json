    "laravel",
    "lumen",
    "laravel5",
    "rabbitmq",
    "amqp",
    "queue",
    "package",
    "message queue",
    "Björn Schmitt",
    "filld"
  ],
  "license": "MIT",
  "support": {
      "issues": "https://github.com/filld/larabbit/issues",
      "source": "https://github.com/filld/larabbit"
  },
  "authors": [
    {
      "name": "Ryan Hoshor",
      "email": "rhoshor@filld.co"
    },
    {
      "name": "Björn Schmitt",
      "email": "code@bjoern.io"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "php-amqplib/php-amqplib": "2.*",
    "illuminate/support": ">=5.1"
  },
  "require-dev": {
